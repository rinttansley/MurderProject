#Select

#SELECTmulCol
SELECT recordid, year, victimsex, weapon
FROM murderstats1st
LIMIT 5;


#SELECTdistinctRows
SELECT DISTINCT weapon
FROM murderstats1st;

#SELECTlimitedrowsstartingrow
SELECT recordid, year, victimsex, weapon
FROM murderstats1st
LIMIT 10 OFFSET 15;

